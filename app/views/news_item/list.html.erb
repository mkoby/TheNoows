<div id="itemList">
  <% if @items.empty? %>
    <h2>No News Items to Display *sad panda*</h2>
  <% else %>
    <% @items.each do |item| %>
      <%= render :partial => 'item', :locals => { :item => item } %>
    <% end %>
    <div class="pageList">
      <%= will_paginate @items %>
    </div
  <% end %>
</div>

